#!/bin/bash

sed -i "s/^\(foreground=\).*/\1'#c0c0fb'/" $HOME/.config/lemonbar/config
sed -i "s/^\(background=\).*/\1'#00000000'/" $HOME/.config/lemonbar/config
sed -i "s/^\(accent=\).*/\1'#4d4dc4'/" $HOME/.config/lemonbar/config
sed -i "s/^\(barfg=\).*/\1'#2f2fbe'/" $HOME/.config/lemonbar/config
sed -i "s/^\(barbg=\).*/\1'#0e0e31'/" $HOME/.config/lemonbar/config
