{
    "search.quickOpen.includeHistory": false,
    "workbench.colorTheme": "Horizon Italic",
    "workbench.startupEditor": "newUntitledFile",
    "workbench.editor.focusRecentEditorAfterClose": false,
    "workbench.tree.renderIndentGuides": "none",
    "workbench.colorCustomizations": {
        "editorIndentGuide.activeBackground": "#888888"
    },
    "breadcrumbs.enabled": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "diffEditor.renderSideBySide": true,
    "editor.cursorSmoothCaretAnimation": true,
    "terminal.integrated.fontFamily": "'Iosevka Term', monospace, 'Droid Sans Fallback'",
    "editor.fontFamily": "Iosevka, monospace, 'Droid Sans Fallback'",
    "editor.fontLigatures": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.renderWhitespace": "boundary",
    "editor.suggest.localityBonus": true,
    "editor.tabSize": 2,
    "editor.wordWrap": "on",
    "editor.wrappingIndent": "indent",
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "name": "No italics",
                "scope": ["keyword.operator", "meta.group.regexp"],
                "settings": {
                    "fontStyle": ""
                }
            }
        ]
    },
    "explorer.openEditors.visible": 99,
    "files.associations": {
        "*.tpp": "cpp",
        "*.s": "gas"
    },
    "files.exclude": {
        "**/.git": false
    },
    "files.enableTrash": false,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.autofetch": true,
    "gitlens.blame.avatars": false,
    "gitlens.blame.toggleMode": "window",
    "gitlens.codeLens.enabled": false,
    "gitlens.heatmap.toggleMode": "window",
    "gitlens.hovers.avatars": false,
    "gitlens.mode.statusBar.enabled": false,
    "gitlens.views.compare.enabled": false,
    "gitlens.views.fileHistory.avatars": false,
    "gitlens.views.lineHistory.enabled": false,
    "gitlens.views.repositories.autoReveal": false,
    "gitlens.views.repositories.showBranchComparison": false,
    "gitlens.views.search.enabled": false,
    "gitlens.keymap": "none",
    "scm.defaultViewMode": "tree",
    "scm.countBadge": "focused",
    "eslint.packageManager": "yarn",
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        { "language": "typescript", "autoFix": true },
        "typescriptreact"
    ],
    "C_Cpp.updateChannel": "Insiders",
    "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: LLVM, UseTab: false, IndentWidth: 2, BreakBeforeBraces: Attach, PointerAlignment: Left, AllowShortIfStatementsOnASingleLine: false, AllowShortBlocksOnASingleLine: false, AllowShortFunctionsOnASingleLine: Empty, AllowShortLoopsOnASingleLine: false, AlwaysBreakTemplateDeclarations: true, IndentCaseLabels: false, ColumnLimit: 80 }",
    "clang-format.fallbackStyle": "{ BasedOnStyle: LLVM, UseTab: false, IndentWidth: 2, BreakBeforeBraces: Attach, PointerAlignment: Left, AllowShortIfStatementsOnASingleLine: false, AllowShortBlocksOnASingleLine: false, AllowShortFunctionsOnASingleLine: Empty, AllowShortLoopsOnASingleLine: false, AlwaysBreakTemplateDeclarations: true, IndentCaseLabels: false, ColumnLimit: 80 }",
    "html.autoClosingTags": false,
    "html.format.extraLiners": "",
    "html.format.indentInnerHtml": true,
    "python.linting.pylintEnabled": true,
    "python.linting.enabled": true,
    "python.formatting.provider": "black",
    "python.terminal.activateEnvironment": false,
    "go.formatTool": "goimports",
    "rust-client.enableMultiProjectSetup": true,
    "extensions.ignoreRecommendations": false,
    "liveshare.featureSet": "insiders",
    "liveshare.nameTagVisibility": "Never",
    "liveshare.openSharedServers": false,
    "liveshare.populateGitCoAuthors": "always",
    "cSpell.language": "en",
    "cSpell.userWords": [
        "convolutional",
        "discriminative",
        "icosahedron",
        "rgba",
        "taha",
        "tokenizes",
        "uikit",
        "vasowalla"
    ],
    "cSpell.allowCompoundWords": true,
    "java.errors.incompleteClasspath.severity": "ignore",
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-shell-escape",
                "-pdf",
                "%DOC%"
            ]
        },
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ]
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": [
                "%DOCFILE%"
            ]
        }
    ],
    "latex-workshop.latexindent.args": [
        "-c",
        "%DIR%/",
        "%TMPFILE%",
        "-y",
        "defaultIndent: '%INDENT%'"
    ],
    "latex-workshop.view.pdf.viewer": "tab",
    "latex-workshop.message.update.show": false,
    "latex-workshop.latex.autoClean.run": "onBuilt",
    "latex-workshop.intellisense.citation.label": "title",
    "latex-workshop.intellisense.citation.type": "browser",
    "latex-workshop.progress.runIconType": "Solid Circled",

    "[python]": {
        "editor.tabSize": 4,
        "editor.defaultFormatter": "ms-python.python"
    },
    "[c]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[rust]": {
        "editor.tabSize": 4
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
}
